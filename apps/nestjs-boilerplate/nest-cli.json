{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "./main.ts",
  "entryFile": "main.js",
  "language": "ts",
  "root": ".",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": {
      "type": "swc",
      "options": {
        "extensions": [".js", ".ts", ".jsx", ".tsx"]
      }
    },
    "watchAssets": true,
    "tsConfigPath": "tsconfig.json"
  }
}
